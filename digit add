

#include <stdio.h>

int main()
{
    //printf("Hello World\n");
    int i=-234;
    int n;
    int s=0;
    do{
        int d=i%10;
        i=i/10;
        
        printf("d=%i\n",d);
        printf("i=%i\n",i);
        s=s+d;
        
    }while(i!=0);
    
    printf("sum=%i",s);
    
    return 0;
}
