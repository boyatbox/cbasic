#include <stdio.h>

int main()
{
   int a,b,c,d,n;
   printf("Enter a number: ");
    scanf("%d",&n);
    for(a=1;a<=n;a++){
        
        printf("\nTable of %d",a);
        for(b=1;b<=10;b++){
        c=a*b;
        printf("\n%dX%d=%d",a,b,c);
            //d=c%10;
            //f(d=0){
            //printf("\n%d's Table:",a);    
            //}
        }
        
    }
    return 0;
}
